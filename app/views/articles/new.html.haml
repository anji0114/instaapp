= form_with(model: @article, url: articles_path, method: 'post', local: true) do |f|
  %div
    %h2= f.label :pictures
  %div
    = f.file_field :pictures, multiple: true
  %div
    %h2= f.label :content, 'content'
  %div
    = f.text_field :content, class: 'text'
  = f.submit '保存', class: 'btn-primary'